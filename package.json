{
  "name": "react-swm-icon-pack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate:icons:broken": "svgr --svgo-config ./svgoConfig.json -d ./src/IconsComponents/broken ./src/assets/icons/broken",
    "generate:icons:curved": "svgr --svgo-config ./svgoConfig.json -d ./src/IconsComponents/curved ./src/assets/icons/curved",
    "generate:icons:duotone": "svgr --svgo-config ./svgoConfig.json -d ./src/IconsComponents/duotone ./src/assets/icons/duotone",
    "generate:icons:outline": "svgr --svgo-config ./svgoConfig.json -d ./src/IconsComponents/outline ./src/assets/icons/outline",
    "generate:icons:twocolor": "svgr --svgo-config ./svgoConfig.json -d ./src/IconsComponents/twocolor ./src/assets/icons/twocolor"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  }
}
